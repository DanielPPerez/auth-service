{
  "ejemplo_registro": {
    "url": "http://localhost:8001/register",
    "method": "POST",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "username": "usuario_prueba",
      "email": "prueba@example.com",
      "password": "MiPassword123!",
      "age": 25,
      "entorno": "casa",
      "nivel_educativo": "secundaria"
    },
    "notas": {
      "entorno_valores_permitidos": [
        "casa",
        "primaria",
        "secundaria",
